      label="VIATRA Query-by-Example (Incubation)"
      version="0.16.2.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <license url="%licenseURL">
      %license
   </license>

   <description>
      Allows the derivation of VIATRA queries based on the current selection in editors.
   </description>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="1.6.2" match="equivalent"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.addon.querybyexample"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.viatra.addon.querybyexample.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
