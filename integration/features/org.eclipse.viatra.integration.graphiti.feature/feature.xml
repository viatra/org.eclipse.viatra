<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.viatra.integration.graphiti.feature"
      label="Graphiti Support for VIATRA (Incubation)"
      version="0.17.0.qualifier"
      provider-name="Eclipse VIATRA Project"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <license url="%licenseURL">
      %license
   </license>

   <description>
      VIATRA Graphiti extensions. Validation framework and query browser support
for various Graphiti editors.
   </description>

   <url>
      <update label="VIATRA" url="http://download.eclipse.org/viatra/updates/release/"/>
   </url>

   <requires>
      <import feature="org.eclipse.viatra.query.sdk.feature" version="1.7.0" match="equivalent"/>
      <import feature="org.eclipse.graphiti.feature"/>
   </requires>

   <plugin
         id="org.eclipse.viatra.integration.graphiti"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
