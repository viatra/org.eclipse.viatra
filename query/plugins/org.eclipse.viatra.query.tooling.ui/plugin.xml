   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.viatra.query.tooling.ui.vql.file">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <adapt
                     type="org.eclipse.core.resources.IFile">
                  <test
                        property="org.eclipse.core.resources.name"
                        value="*.vql">
                  </test>
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.root">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra.query.tooling.ui.queryexplorer.content.matcher.PatternMatcherRootContent">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.match">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra.query.tooling.ui.queryexplorer.content.matcher.PatternMatchContent">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.matcher">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra.query.tooling.ui.queryexplorer.content.matcher.PatternMatcherContent">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.patterncomponent">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra.query.tooling.ui.queryexplorer.content.patternsviewer.PatternComponent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.activedomaineditor">
         <with
               variable="activeEditor">
            <instanceof
                  value="org.eclipse.ui.IEditorPart">
            </instanceof>
            <adapt
                  type="org.eclipse.viatra.query.tooling.ui.queryexplorer.IModelConnector">
            </adapt>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.activepatterneditor">
         <with
               variable="activeEditorId">
            <equals
                  value="org.eclipse.viatra.query.patternlanguage.emf.EMFPatternLanguage">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra.query.tooling.ui.filteredmatchercontent">
            <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra.query.tooling.ui.util.IFilteredMatcherContent">
               </instanceof>
            </iterate>
         </with>
      </definition>
   </extension>
